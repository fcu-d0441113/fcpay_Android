<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.root.fcpay">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="FcPay"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Login.HomeViewController">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Login.OAuthLoginViewController" />
        <activity android:name=".Order.OrderTable.OrderTableViewController" />
        <activity android:name=".Order.OrderTable.OrderTableViewModel" />
        <activity android:name=".Order.OrderDetail.OrderDetailUIViewModel" />
        <activity android:name=".Order.OrderDetail.OrderDetailUIViewController" />
        <activity android:name=".Order.PlentyOrder.OrderPlentyViewController" />
        <activity android:name=".Payment.SunnyBankPaymentVM" />
        <activity android:name=".Payment.SunnyBankPaymentVC" />
        <activity android:name=".Order.OrderRecord.OrderRecordViewModel" />
        <activity android:name=".Order.OrderRecord.OrderRecordViewController" />
        <activity android:name=".Profile.UserProfileViewController"></activity>
    </application>

</manifest>